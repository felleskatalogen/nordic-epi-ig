    <!-- Using Norwegian designations in display because the required slices are not recognized otherwise -->
    <concept>
        <code value="001"/>
        <display value="svært vanlige"/>
        <definition value="(≥1/10)"/>
        <designation>
            <language value="en"/>
            <value value="Very common"/>
        </designation>
        <designation>
            <language value="no"/>
            <value value="svært vanlige"/>
        </designation>
    </concept>
    <concept>
        <code value="002"/>
        <display value="vanlige"/>
        <definition value="≥1/100 to &lt;1/10"/>
        <designation>
            <language value="en"/>
            <value value="Common"/>
        </designation>
        <designation>
            <language value="no"/>
            <value value="vanlige"/>
        </designation>
    </concept>
    <concept>
        <code value="003"/>
        <display value="Uncommon"/>
        <definition value="≥1/1000 to &lt;1/100"/>
