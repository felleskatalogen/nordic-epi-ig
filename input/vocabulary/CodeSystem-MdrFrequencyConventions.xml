            <language value="en"/>
            <value value="mindre vanlige"/>
        </designation>
        <designation>
            <language value="no"/>
            <value value="mindre vanlige"/>
        </designation>
    </concept>
    <concept>
        <code value="004"/>
        <display value="sjeldne"/>
